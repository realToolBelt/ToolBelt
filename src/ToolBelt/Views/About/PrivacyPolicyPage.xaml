<?xml version="1.0" encoding="utf-8" ?>
<vw:ContentPageBase x:Class="ToolBelt.Views.About.PrivacyPolicyPage"
                    xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:local="clr-namespace:ToolBelt.Views.About"
                    xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                    xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                    xmlns:vw="clr-namespace:ToolBelt.Views"
                    prism:ViewModelLocator.AutowireViewModel="True"
                    x:TypeArguments="local:PrivacyPolicyPageViewModel">
    <ContentPage.Content>
        <StackLayout>
            <Label FontSize="{StaticResource LargeSize}"
                   HorizontalOptions="CenterAndExpand"
                   Text="Privacy Policy..."
                   VerticalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</vw:ContentPageBase>
<?xml version="1.0" encoding="utf-8" ?>
<rxui:ReactiveContentView x:Class="ToolBelt.Views.ReviewsPage"
                          xmlns="http://xamarin.com/schemas/2014/forms"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns:controls="clr-namespace:ToolBelt.Controls"
                          xmlns:local="clr-namespace:ToolBelt.Views"
                          xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                          xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                          prism:ViewModelLocator.AutowireViewModel="True"
                          x:TypeArguments="local:ReviewsPageViewModel">
    <StackLayout>
        <Label FontSize="{StaticResource LargeSize}"
               HorizontalOptions="CenterAndExpand"
               Text="Reviews..."
               VerticalOptions="CenterAndExpand" />
    </StackLayout>
</rxui:ReactiveContentView>